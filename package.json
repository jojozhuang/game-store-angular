  "license": "MIT",
  "scripts": {
    "start": "ng serve",
    "start:prod": "ng serve --configuration production",
    "start:azure": "ng serve --configuration azure",
    "start:netlify": "ng serve --configuration netlify",
    "start:render": "ng serve --configuration render",
    "build": "ng build --configuration production",
    "build:azure": "ng build --configuration azure",
    "build:netlify": "ng build --configuration netlify",
    "build:render": "ng build --configuration render",
    "build:pages": "ng build --configuration pages --base-href /game-store-angular/",
    "test": "ng test",
    "test:headless": "ng test --watch=false --browsers=ChromeHeadless",
    "test:coverage": "ng test --karma-config=karma.conf.js --no-watch --code-coverage",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "e2e": "npx cypress open",
    "e2e:headless": "npx cypress run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.6",
    "@angular/common": "^20.3.6",
    "@angular/compiler": "^20.3.6",
    "@angular/core": "^20.3.6",
    "@angular/forms": "^20.3.6",
    "@angular/platform-browser": "^20.3.6",
    "@angular/platform-browser-dynamic": "^20.3.6",
    "@angular/router": "^20.3.6",
    "bootstrap": "^4.6.2",
    "ngx-bootstrap": "^20.0.2",
    "rxjs": "^7.8.0",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular/build": "^20.3.6",
    "@angular/cli": "20.3.13",
    "@angular/compiler-cli": "20.3.6",
    "@cypress/schematic": "2.4.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.38.0",
    "@types/jasmine": "3.10.3",
    "angular-eslint": "20.4.0",
    "cypress": "15.6.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jasmine-core": "~5.1.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.6.2",
    "typescript": "5.8.3",
    "typescript-eslint": "8.46.0"
  },
  "engines": {
    "node": "20.19.5",
    "npm": "10.8.2"
  }
